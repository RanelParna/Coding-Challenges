 RowBox[{
  RowBox[{"TheMagicSquareQ", "[", "M_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "dims", ",", "targetValue", ",", "targetArray", ",", "MT", ",", 
      "colsTotals", ",", "rowsTotals", ",", "totalDiag", ",", "totalAntiDiag",
       ",", "size", ",", "listOfRequiredValues", ",", 
      "sortedOneDimListOfMatrixElements"}], "}"}], ",", 
    RowBox[{
     RowBox[{"Print", "[", "\"\<Tere!\>\"", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"dims", "=", 
      RowBox[{"Dimensions", "[", "M", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<dims=\>\"", ",", "dims"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "Check", " ", "that", " ", "we", " ", "have", " ", "a", " ", 
        "Matrix"}], ",", 
       RowBox[{
        RowBox[{"i", ".", "e"}], " ", "a", " ", "2", "x2", " ", "array"}]}], 
      "*)"}], 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "dims", "]"}], "!=", "2"}], ",", 
       RowBox[{"Return", "[", "False", "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Check", " ", "that", " ", "the", " ", "matrix", " ", "is", " ", "a", 
       " ", "square", " ", "matrix"}], "*)"}], 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"dims", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "!=", 
        RowBox[{"dims", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], ",", 
       RowBox[{"Return", "[", "False", "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"size", " ", "of", " ", "square", " ", "matrix"}], "*)"}], 
     RowBox[{"size", "=", 
      RowBox[{"dims", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<size=\>\"", ",", "size"}], "]"}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"listOfRequiredValues", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"i", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"size", "^", "2"}]}], "}"}]}], "]"}]}], ";", "\n", 
     RowBox[{"Print", "[", "listOfRequiredValues", "]"}], ";", "\n", 
