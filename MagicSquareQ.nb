     RowBox[{
     "Print", "[", "\"\<Maatriks ongi vastuv\[OTilde]etav!\>\"", "]"}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"targetValue", "=", 
      RowBox[{"Total", "[", 
       RowBox[{"M", "[", 
        RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Copy", " ", "the", " ", "value", " ", "size", " ", "times", " ", "in", 
       " ", "an", " ", "array"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"targetArray", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"targetValue", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "size"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Creating", " ", "a", " ", "list", " ", "of", " ", "sums", " ", "of", 
